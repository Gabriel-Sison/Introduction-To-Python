print('How many columns do you want? ', end='')
columns = int(input())
print('How many rows do you want? ', end='')
rows = int(input())
print('x', end="\t")
for h in range(1, (columns + 1)):
    print(h, end=" \t")
print()
num = 1
mult = 1
for i in range(rows):
    print(num, end='\t')
    for j in range(columns ):
        print(num * mult, end='\t')
        mult += 1
    print()
    num += 1
    mult = 1
